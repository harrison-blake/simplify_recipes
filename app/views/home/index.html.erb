<html>
  <head>
    <title>Simplify</title>
  </head>
  <body>
    <h1>Simplify</h1>
    <div>
      <%= form_with model: Recipe, url: recipes_path, method: :post, local: true do |f| %>
        <div>
          <%= f.label :url %><br>
          <%= f.text_field :url %>
        </div>

        <div>
          <%= f.submit "Submit" %>
        </div>
      <% end %>
    </div>
  </body>
</html>
